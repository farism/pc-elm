{
  "name": "procore-elm",
  "version": "1.0.0",
  "description": "A frontend client for Procore",
  "main": "index.js",
  "scripts": {
    "build": "npm run clean && NODE_ENV=production webpack",
    "clean": "rm -rf dist/",
    "dev": "NODE_ENV=development webpack-dev-server",
    "elm-format": "elm-format src/ --yes",
    "eslint": "eslint \"{webpack.config.js,src/**/*.js}\" --fix",
    "format": "npm run elm-format && npm run prettier && npm run eslint",
    "prettier": "prettier --single-quote --trailing-comma es5 --write \"{webpack.config.js,src/**/*.js}\"",
    "restage": "git update-index --again",
    "test": "echo \"Error: no test specified\" && exit 0"
  },
  "pre-commit": [
    "test",
    "format",
    "restage"
  ],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "babel": "^6.5.2",
    "babel-cli": "^6.14.0",
    "babel-core": "^6.14.0",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.2.5",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-polyfill": "^6.13.0",
    "babel-preset-latest": "^6.14.0",
    "babel-register": "^6.14.0",
    "chunk-manifest-webpack-plugin": "^1.0.0",
    "compression-webpack-plugin": "^0.3.2",
    "elm-assets-loader": "^0.2.0",
    "elm-hot-loader": "^0.5.4",
    "elm-webpack-loader": "^4.2.0",
    "eslint": "^3.17.1",
    "eslint-plugin-babel": "^4.1.1",
    "file-loader": "^0.10.1",
    "html-webpack-plugin": "^2.28.0",
    "pre-commit": "^1.2.2",
    "prettier": "^0.22.0",
    "prettier-eslint": "^4.2.1",
    "webpack": "^2.2.1",
    "webpack-dev-server": "^2.4.1",
    "webpack-manifest-plugin": "^1.1.0"
  },
  "dependencies": {}
}
